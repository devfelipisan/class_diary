const skillsArray: Array<fieldscommonDTO> = [];

async function main() {
  competencesArray.map(async (competence: competencesdto) => {
    await prisma.competences.create({
      data: {
        id: competence.id,
        name: competence.name,
        created: new Date(Date()).toISOString(),
      },
    });
    console.log(competence.name);
  });
  groupYearArray.map(async (groupYear: groupYearDto) => {
    await prisma.group_years.create({
      data: {
        id: groupYear.id,
        description: groupYear.description,
        created: new Date(Date()).toISOString(),
      },
    });
    console.log(groupYear.description);
  });
  objectsArray.map(async (object: fieldscommonDTO) => {
    await prisma.objects.create({
      data: {
        description: object.description,
        created: new Date(Date()).toISOString(),
      },
    });
    console.log(object.description);
  });
  unitsArray.map(async (unit: fieldscommonDTO) => {
    await prisma.units.create({
      data: {
        description: unit.description,
        created: new Date(Date()).toISOString(),
      },
    });
    console.log(unit.description);
  });
}
main()
  .then(async () => {
    await prisma.$disconnect();
  })
  .catch(async (e) => {
    console.error(e);
    await prisma.$disconnect();
    process.exit(1);
  });
